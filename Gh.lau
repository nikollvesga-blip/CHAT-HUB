-- Funci√≥n para crear botones
local function crearBoton(nombre, posicion, tama√±o, callback)
    local button = Instance.new("TextButton")
    button.Text = nombre
    button.Size = UDim2.new(0, tama√±o.X, 0, tama√±o.Y)
    button.Position = UDim2.new(0, posicion.X, 0, posicion.Y)
    button.BackgroundColor3 = Color3.fromRGB(255, 215, 0) -- dorado
    button.TextColor3 = Color3.new(0,0,0)
    button.Parent = screenGui
    button.MouseButton1Click:Connect(callback)
end

